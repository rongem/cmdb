        </sectionGroup>
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/>
    </startup>
    <userSettings>
        <RZManager.Properties.Settings>
            <setting name="MinimumHeight" serializeAs="String">
                <value>3</value>
            </setting>
        </RZManager.Properties.Settings>
    </userSettings>
  <system.serviceModel>
