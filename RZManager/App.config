      <webHttpEndpoint>
        <standardEndpoint name="MyEndPoint" transferMode="Buffered" faultExceptionEnabled="false" automaticFormatSelectionEnabled="false" maxReceivedMessageSize="25600000">
          <security mode="TransportCredentialOnly">
            <transport clientCredentialType="Windows"/>
          </security>
        </standardEndpoint>
      </webHttpEndpoint>
    </standardEndpoints>
    <behaviors>
      <endpointBehaviors>
        <behavior name="MyEndPointBehavior">
          <webHttp defaultBodyStyle="Wrapped"/>
        </behavior>
      </endpointBehaviors>
    </behaviors>
    <client>
      <!--http://localhost:51717/-->
      <endpoint address="http://localhost" name="CmsService" binding="webHttpBinding" contract="CmsService.ICmsService" behaviorConfiguration="MyEndPointBehavior" kind="webHttpEndpoint" endpointConfiguration="MyEndPoint"/>
    </client>
  </system.serviceModel>
  <applicationSettings>
    <RZManager.Properties.Settings>
      <setting name="BlockedUnitsFile" serializeAs="String">
        <value>BlockedUnits.xml</value>
      </setting>
      <setting name="EnclosureTypesFile" serializeAs="String">
        <value>EnclosureTypes.xml</value>
      </setting>
      <setting name="SystemsFile" serializeAs="String">
        <value>Systems.xml</value>
      </setting>
      <setting name="SettingsFile" serializeAs="String">
        <value>Settings.xml</value>
      </setting>
      <setting name="WebGuiPath" serializeAs="String">
        <value>/ShowItem.aspx?id={0}</value>
      </setting>
      <setting name="WebServicePath" serializeAs="String">
        <value>/API/CmsService.svc</value>
