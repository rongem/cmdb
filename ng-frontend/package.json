{
  "name": "ng-frontend",
  "version": "2.1.128",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:all": "ng build --configuration production backend-access && ng build --configuration=production,en cmdb && ng build --configuration production dcman",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docker": "docker build -t rongem/cmdb-frontend .",
    "dev:backend": "ng build backend-access",
    "dev:cmdb": "ng s --host 0.0.0.0 cmdb",
    "dev:dcman": "ng s --host 0.0.0.0 dcman"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.9",
    "@angular/cdk": "^18.2.10",
    "@angular/common": "^18.2.9",
    "@angular/compiler": "^18.2.9",
    "@angular/core": "^18.2.9",
    "@angular/forms": "^18.2.9",
    "@angular/localize": "^18.2.9",
    "@angular/platform-browser": "^18.2.9",
    "@angular/platform-browser-dynamic": "^18.2.9",
    "@angular/router": "^18.2.9",
    "@ngrx/effects": "^18.1.0",
    "@ngrx/operators": "^18.1.0",
    "@ngrx/router-store": "^18.1.0",
    "@ngrx/store": "^18.1.0",
    "@ngrx/store-devtools": "^18.1.0",
    "rxjs": "^7.4.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-latest/xlsx-latest.tgz",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.10",
    "@angular/cli": "^18.2.10",
    "@angular/compiler-cli": "^18.2.9",
    "@angular/language-service": "^18.2.9",
    "@types/jest": "^29.5.13",
    "@types/node": "^12.20.37",
    "grunt": "^1.5.2",
    "grunt-bump": "^0.8.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.2.4",
    "ng-packagr": "^18.0.0",
    "typescript": "~5.4.5"
  }
}
