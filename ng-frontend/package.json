{
  "name": "ng-frontend",
  "version": "2.1.154",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:all": "ng build --configuration production backend-access && ng build --configuration=production,en cmdb && ng build --configuration production dcman",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docker": "docker build -t rongem/cmdb-frontend .",
    "dev:backend": "ng build backend-access",
    "dev:cmdb": "ng s --host 0.0.0.0 cmdb",
    "dev:dcman": "ng s --host 0.0.0.0 dcman"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.10",
    "@angular/cdk": "^19.2.16",
    "@angular/common": "^19.2.10",
    "@angular/compiler": "^19.2.10",
    "@angular/core": "^19.2.10",
    "@angular/forms": "^19.2.10",
    "@angular/localize": "^19.2.10",
    "@angular/platform-browser": "^19.2.10",
    "@angular/platform-browser-dynamic": "^19.2.10",
    "@angular/router": "^19.2.10",
    "@ngrx/effects": "^19.2.0",
    "@ngrx/operators": "^19.2.0",
    "@ngrx/router-store": "^19.2.0",
    "@ngrx/store": "^19.2.0",
    "@ngrx/store-devtools": "^19.2.0",
    "rxjs": "^7.4.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-latest/xlsx-latest.tgz",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.12",
    "@angular/cli": "^19.2.12",
    "@angular/compiler-cli": "^19.2.10",
    "@angular/language-service": "^19.2.10",
    "@types/jest": "^29.5.13",
    "@types/node": "^12.20.37",
    "grunt": "^1.5.2",
    "grunt-bump": "^0.8.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.2.4",
    "ng-packagr": "^19.0.1",
    "typescript": "~5.6.3"
  }
}
