{
  "name": "ng-frontend",
  "version": "2.1.163",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:all": "ng build --configuration production backend-access && ng build --configuration=production,en cmdb && ng build --configuration production dcman",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docker": "docker build -t rongem/cmdb-frontend .",
    "dev:backend": "ng build backend-access",
    "dev:cmdb": "ng s --host 0.0.0.0 cmdb",
    "dev:dcman": "ng s --host 0.0.0.0 dcman"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.3",
    "@angular/cdk": "^20.2.7",
    "@angular/common": "^20.3.3",
    "@angular/compiler": "^20.3.3",
    "@angular/core": "^20.3.3",
    "@angular/forms": "^20.3.3",
    "@angular/localize": "^20.3.3",
    "@angular/platform-browser": "^20.3.3",
    "@angular/platform-browser-dynamic": "^20.3.3",
    "@angular/router": "^20.3.3",
    "@ngrx/effects": "^20.0.1",
    "@ngrx/operators": "^20.0.1",
    "@ngrx/router-store": "^20.0.1",
    "@ngrx/store": "^20.0.1",
    "@ngrx/store-devtools": "^20.0.1",
    "rxjs": "^7.4.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-latest/xlsx-latest.tgz",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.3",
    "@angular/cli": "^20.3.3",
    "@angular/compiler-cli": "^20.3.3",
    "@angular/language-service": "^20.3.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "grunt": "^1.5.2",
    "grunt-bump": "^0.8.0",
    "jest": "^30.0.5",
    "jest-preset-angular": "^15.0.0",
    "ng-packagr": "^20.1.0",
    "typescript": "~5.8.3"
  }
}
