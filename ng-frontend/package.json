{
  "name": "ng-frontend",
  "version": "2.1.174",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:all": "ng build --configuration production backend-access && ng build --configuration=production,en cmdb && ng build --configuration production dcman",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docker": "docker build -t rongem/cmdb-frontend .",
    "dev:backend": "ng build backend-access",
    "dev:cmdb": "ng s --host 0.0.0.0 cmdb",
    "dev:dcman": "ng s --host 0.0.0.0 dcman"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.1.0",
    "@angular/cdk": "^21.1.0",
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/localize": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/platform-browser-dynamic": "^21.1.0",
    "@angular/router": "^21.1.0",
    "@ngrx/effects": "^21.0.1",
    "@ngrx/operators": "^21.0.1",
    "@ngrx/router-store": "^21.0.1",
    "@ngrx/store": "^21.0.1",
    "@ngrx/store-devtools": "^21.0.1",
    "rxjs": "^7.4.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-latest/xlsx-latest.tgz",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.0",
    "@angular/cli": "^21.1.0",
    "@angular/compiler-cli": "^21.1.0",
    "@angular/language-service": "^21.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "jest": "^30.0.5",
    "jest-preset-angular": "^16.0.0",
    "ng-packagr": "^21.0.1",
    "typescript": "~5.9.3"
  }
}
