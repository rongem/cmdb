{
  "name": "njs-backend",
  "version": "0.8.1",
  "description": "CMDB backend on NodeJS",
  "main": "./dist/app.ts",
  "scripts": {
    "test": "mocha --timeout 5000",
    "prep": "tsc --declaration && mocha --timeout 5000 && grunt bump",
    "comp": "tsc --declaration",
    "dev": "ts-node-dev -r dotenv/config --respawn --transpile-only ./src/app.ts",
    "prod": "tsc && node ./build/app.js",
    "precomp": "tslint -c tslint.json -p tsconfig.json --fix",
    "docker": "docker build .",
    "start": "node dist/app.js"
  },
  "keywords": [
    "CMDB",
    "Configuration management"
  ],
  "author": "Michael Ronge",
  "license": "Apache license 2.0",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-ntlm": "^2.5.2",
    "express-validator": "^6.10.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.18",
    "multer": "^1.4.2",
    "socket.io": "^3.1.2",
    "xlsx": "^0.16.9"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.11",
    "@types/express-ntlm": "^2.3.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.6.8",
    "@types/mongoose": "^5.10.3",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.31",
    "@types/socket.io": "^2.1.13",
    "chai": "^4.3.3",
    "chai-http": "^4.3.0",
    "dotenv": "^8.2.0",
    "grunt": "^1.3.0",
    "grunt-bump": "^0.8.0",
    "mocha": "^8.3.0",
    "sinon": "^9.2.4",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  }
}
