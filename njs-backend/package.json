{
  "name": "njs-backend",
  "version": "1.0.176",
  "description": "CMDB backend on NodeJS",
  "main": "./dist/indext.ts",
  "scripts": {
    "test": "jest --no-cache --detectOpenHandles --covarage=true --runInBand",
    "prep": "tsc --declaration && mocha -r ts-node/register test/**/*.ts --timeout 5000 && grunt bump",
    "comp": "tsc --declaration",
    "dev": "nodemon -r dotenv/config ./src/index.ts",
    "prod": "tsc && node ./build/index.js",
    "docker": "docker build -t rongem/cmdb-backend .",
    "start": "node dist/index.js"
  },
  "jest": {
    "preset": "ts-jest",
    "testSequencer": "./src/test/sequencer.js",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./src/test/setup.ts"
    ],
    "globalTeardown": "./src/test/teardown.ts"
  },
  "keywords": [
    "CMDB",
    "Configuration management"
  ],
  "author": "Michael Ronge",
  "license": "Apache license 2.0",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.0",
    "express": "^5.1.0",
    "express-ntlm": "^2.6.1",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "8.19",
    "multer": "^2.0.0",
    "socket.io": "^4.4.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-latest/xlsx-latest.tgz"
  },
  "devDependencies": {
    "@jest/test-sequencer": "^30.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.1",
    "@types/express-ntlm": "^2.3.2",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.1",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.2",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.0",
    "jest": "^30.0.0",
    "supertest": "^7.0.0",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.2"
  }
}
