{
  "name": "njs-backend",
  "version": "1.0.0",
  "description": "CMDB backend on NodeJS",
  "main": "./dist/app.ts",
  "scripts": {
    "test": "mocha --timeout 5000",
    "ct": "tsc && mocha --timeout 5000",
    "prep": "tsc --declaration && mocha --timeout 5000 && grunt bump",
    "comp": "tsc --declaration",
    "dev": "ts-node-dev -r dotenv/config --respawn --transpile-only ./src/app.ts",
    "prod": "tsc && node ./build/app.js",
    "docker": "docker build -t rongem/cmdb-backend .",
    "start": "node dist/app.js"
  },
  "keywords": [
    "CMDB",
    "Configuration management"
  ],
  "author": "Michael Ronge",
  "license": "Apache license 2.0",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "express-ntlm": "^2.6.1",
    "express-validator": "^6.13.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "5.13.7",
    "multer": "^1.4.3",
    "socket.io": "^4.3.0",
    "swagger-ui-express": "^4.1.6",
    "xlsx": "^0.17.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.12",
    "@types/express-ntlm": "^2.3.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/multer": "^1.4.6",
    "@types/node": "^14.17.27",
    "@types/swagger-ui-express": "^4.1.2",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "dotenv": "^10.0.0",
    "grunt": "^1.4.1",
    "grunt-bump": "^0.8.0",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  }
}
